@model UpdateRoleDto
@{
    ViewData["Title"] = "UpdateRole";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<h1>Rol Güncelleme Sayfası</h1>


<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Role Adı</label>
    <input type="text" class="form-control" asp-for="Name" />
    <br />
    <button class="btn mb-1 btn-rounded btn-outline-primary">Kaydet</button>
</form>

